{
    "name": "osca",
    "private": true,
    "description": "OSCA",
    "license": "UNLICENSED",
    "engines": {
        "node": ">=18.18.2"
    },
    "cacheDirectories": [
        "node_modules"
    ],
    "scripts": {
        "build": "vue-tsc -p tsconfig.build.json --noEmit && vite build --emptyOutDir",
        "dev": "vite",
        "lint": "eslint --ext .js,.ts,.vue src",
        "preview": "vite preview",
        "start": "vite",
        "test": "vitest run --coverage",
        "test:watch": "vitest --",
        "prepare": "husky install",
        "prettier:check": "prettier --check '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}'",
        "prettier:format": "prettier --write '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}'"
    },
    "devDependencies": {
        "@pinia/testing": "0.1.3",
        "@prettier/plugin-xml": "3.2.1",
        "@rushstack/eslint-patch": "1.5.1",
        "@types/sinon": "10.0.19",
        "@typescript-eslint/parser": "6.8.0",
        "@vitejs/plugin-vue": "4.4.0",
        "@vitest/coverage-istanbul": "0.34.6",
        "@vue/eslint-config-prettier": "8.0.0",
        "@vue/eslint-config-typescript": "12.0.0",
        "@vue/test-utils": "2.4.1",
        "axios-mock-adapter": "^1.22.0",
        "eslint": "8.51.0",
        "eslint-plugin-vue": "9.17.0",
        "husky": "8.0.3",
        "jsdom": "22.1.0",
        "lint-staged": "15.0.2",
        "prettier": "3.0.3",
        "prettier-plugin-java": "2.3.1",
        "prettier-plugin-packagejson": "2.4.6",
        "sass": "^1.70.0",
        "sinon": "16.1.3",
        "typescript": "5.2.2",
        "vite": "4.5.2",
        "vitest": "0.34.6",
        "vitest-sonar-reporter": "0.4.1",
        "vue-tsc": "1.8.19"
    },
    "dependencies": {
        "axios": "1.6.1",
        "pinia": "2.1.7",
        "pinia-plugin-persistedstate": "^3.2.0",
        "primeflex": "3.3.1",
        "primeicons": "6.0.1",
        "primevue": "3.35.0",
        "vee-validate": "4.11.8",
        "vue": "3.3.4",
        "vue-i18n": "9.4.1",
        "vue-router": "4.2.5"
    }
}
